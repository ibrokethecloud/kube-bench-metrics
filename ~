apiVersion: monitoring.coreos.com/v1
kind: ServiceMonitor
metadata:
  labels:
    app: kube-bench-metrics
  name: kube-bench-metrics-monitoring
  namespace: cattle-prometheus
spec:
  endpoints:
  - port: metrics
    interval: 1h
  namespaceSelector:
    matchNames:
    - kube-bench-metrics
  selector:
    matchLabels:
      app.kubernetes.io/name: kube-bench-metrics
